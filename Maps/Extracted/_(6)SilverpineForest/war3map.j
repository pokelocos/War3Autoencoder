function main takes nothing returns nothing
    call SetCameraBounds( -8704.0 + GetCameraMargin(CAMERA_MARGIN_LEFT), -8960.0 + GetCameraMargin(CAMERA_MARGIN_BOTTOM), 8704.0 - GetCameraMargin(CAMERA_MARGIN_RIGHT), 8448.0 - GetCameraMargin(CAMERA_MARGIN_TOP), -8704.0 + GetCameraMargin(CAMERA_MARGIN_LEFT), 8448.0 - GetCameraMargin(CAMERA_MARGIN_TOP), 8704.0 - GetCameraMargin(CAMERA_MARGIN_RIGHT), -8960.0 + GetCameraMargin(CAMERA_MARGIN_BOTTOM) )
    call SetDayNightModels( "Environment\\DNC\\DNCLordaeron\\DNCLordaeronTerrain\\DNCLordaeronTerrain.mdl", "Environment\\DNC\\DNCLordaeron\\DNCLordaeronUnit\\DNCLordaeronUnit.mdl" )
    call NewSoundEnvironment( "Default" )
    call SetAmbientDaySound( "LordaeronWinterDay" )
    call SetAmbientNightSound( "LordaeronWinterNight" )
    call SetMapMusic( "Music", true, 0 )
    call CreateAllUnits(  )
    call InitBlizzard(  )
    call InitGlobals(  )
    call InitCustomTriggers(  )
    call RunInitializationTriggers(  )

endfunction

